{
  "name": "shrtr",
  "version": "0.0.1",
  "description": "Encurtador de links",
  "scripts": {
    "install": "yarn --cwd ./shrtrr install",
    "install:all": "yarn --cwd ./shrtr install && yarn --cwd ./shrtrr install",
    "start": "yarn --cwd ./shrtrr start",
    "start:all": "yarn --cwd ./shrtr start & yarn --cwd ./shrtrr start",
    "upgrade": "yarn --cwd ./shrtrr upgrade --latest",
    "upgrade:all": "yarn --cwd ./shrtr upgrade --latest && yarn --cwd ./shrtrr upgrade --latest",
    "build": "yarn --cwd ./shrtrr build",
    "test": "yarn --cwd ./shrtrr test",
    "test:all": "yarn --cwd ./shrtr test & yarn --cwd ./shrtrr test",
    "coverage": "yarn --cwd ./shrtrr coverage",
    "coverage:all": "yarn --cwd ./shrtr coverage & yarn --cwd ./shrtrr coverage",
    "twatch": "yarn --cwd ./shrtrr twatch"
  },
  "author": "Caio Campos",
  "license": "MIT"
}
